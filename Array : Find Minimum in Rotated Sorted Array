class Solution {
    public int findMin(int[] nums) {
        int start = 0; 
        int last = nums.length - 1;

        while(start < last) {
            int mid = (start + last) / 2;

            if(nums[mid] > nums[last]) {
                start = mid + 1;
            } else {
                last = mid;
            }
        }

        return nums[start];
    }
}
