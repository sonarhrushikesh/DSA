class Solution {
    public int removeDuplicates(int[] arr) {
       
        int unique = 1;
        
        for (int i=0; i<arr.length-1; i++) {
            
            if (arr[i] < arr[i+1]) {
                arr[unique] = arr[i+1];
                unique++;
            }
        }
        
        return unique;
    }
}
